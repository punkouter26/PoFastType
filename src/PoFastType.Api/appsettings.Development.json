{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Information",
        "PoFastType.Api.Middleware": "Debug",
        "PoFastType.Api.Controllers": "Debug"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "PoFastType.Api.Middleware": "Debug",
      "PoFastType.Api.Controllers": "Debug"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Debug"
      }
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "DebugSettings": {
    "EnableClientLogging": true,
    "EnableNetworkMonitoring": true,
    "EnableVerboseLogging": true,
    "LogPath": "../DEBUG/log.txt",
    "ClientLogPath": "../DEBUG/client-logs.txt",
    "NetworkLogPath": "../DEBUG/network-activity.txt",
    "OverwriteLogOnStartup": true,
    "MinimumLogLevel": "Debug"
  },
  "AzureTableStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "TableName": "PoFastTypeGameResults"
  },
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "AllowedHosts": "*"
}
