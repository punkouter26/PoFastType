{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "IncludeScopes": true
    },
    "File": {
      "Path": "log.txt",
      "Append": false,
      "MinLevel": "Information",
      "FileSizeLimitBytes": 10485760,
      "MaxRollingFiles": 1
    }
  },
  "AzureTableStorage": {
    "ConnectionString": "UseDevelopmentStorage=true",
    "TableName": "PoFastTypeGameResults"
  },
  "AllowedHosts": "*"
}
